<?xml version="1.0"?>
<librec-auto>

	<!-- ALGORITHM SECTION -->
	<alg id="itemknn">
		<param name="rec.recommender.similarities">item</param>
		<param name="rec.similarity.class">pcc</param>
		<param name="rec.neighbors.knn.number">25</param>
		<param name="rec.similarity.shrinkage">5</param>
	</alg>
	<!-- EVALUATION SECTION -->
	<metric id="rmse">
		<param name="rec.eval.classes">rmse</param>
		<param name="rec.recommender.isranking">false</param>
	</metric>

</librec-auto>
